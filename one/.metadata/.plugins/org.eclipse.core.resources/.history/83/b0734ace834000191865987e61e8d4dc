import java.awt.GridLayout;

public class ClientLogin extends JFrame{
	
	JLabel jlbl1;
	
	
	
	public ClientLogin() {
		jlbl1=new JLabel(new ImageIcon("images/tou.gif"));
		this.add(jlbl1,"North");
		
